extend layout

block content
    h1 Create a new record #{patientId}
    form(action="/practitioner/createRecord", method="POST", enctype="multipart/form-data")
        input#fileinput(type='file', name='record')
        br
        input(type='text', placeholder='description', name='description', size='30')
        br
        input(type='hidden', name='patientId', value=patientId)
        input(type='submit' value = 'submit')
        